extends ../../layouts/default

block styles
    link(rel="stylesheet" href!=`${baseDir}css/main.css?v=${Date.now()}`)
    link(rel="stylesheet" href!=`${baseDir}css/partners.css?v=${Date.now()}`)
    link(rel="stylesheet" href!="https://unpkg.com/swiper@6.3.2/swiper-bundle.min.css")
    link(rel="stylesheet" href!="https://cdn.jsdelivr.net/npm/glightbox/dist/css/glightbox.min.css")
block content
    .partners
        include particles/partners-header
        include particles/partners-events
        include particles/partners-partners
       
block scripts
    script(src!=`${baseDir}js/common.js?v=${Date.now()}`)
    script(defer src!="https://unpkg.com/swiper/swiper-bundle.min.js")
    script(defer src!="https://cdn.jsdelivr.net/gh/mcstudios/glightbox/dist/js/glightbox.min.js")
    script(defer src!=`${baseDir}js/partners.js?v=${Date.now()}`)
